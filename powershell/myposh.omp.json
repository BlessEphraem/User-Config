{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "background": "#FF1797",
          "foreground": "#000000",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "style": "diamond",
          "template": " @{{ .UserName }}",
          "type": "session"
        },
        {
          "foreground": "#0CA0D8",
          "properties": {
            "folder_separator_icon": "/",
            "style": "full"
          },
          "style": "plain",
          "template": " \uf07b {{ .Path }} ",
          "type": "path"
        }
      ]
    },
    {
      "type": "prompt",
      "newline": false,
      "alignment": "right",
      "segments": [
        {
          "foreground": "#14A5AE",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "fetch_upstream_icon": true
          },
          "style": "plain",
          "template": "{{ .UpstreamIcon }}{{ .HEAD }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "type": "git"
        }
      ]
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#FF1797",
          "style": "plain",
          "template": "❯ ",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 3
}