{
  "$schema": "https://raw.githubusercontent.com/LGUG2Z/komorebi/v0.1.37/schema.json",
  "minimum_window_width": 0,
  "minimum_window_height": 0,
  "resize_delta": 500,
  "window_container_behaviour": "Create",
  "float_override": false,
  "floating_layer_behaviour": "Tile",
  "toggle_float_placement": "CenterAndResize",
  "floating_layer_placement": "CenterAndResize",
  "float_override_placement": "CenterAndResize",
  "float_rule_placement": "Center",
  "cross_monitor_move_behaviour": "Insert",
  "cross_boundary_behaviour": "Workspace",
  "mouse_follows_focus": false,
  "border_width": 6,
  "animation": {
    "enabled": true,
    "duration": 100,
    "fps": 100,
    "style": "EaseOutSine"
  },
  "border_colours": {
    "single" : { "r": 255, "g": 23, "b": 150 },
    "stack" : { "r": 255, "g": 23, "b": 150 },
    "monocle" : { "r": 255, "g": 23, "b": 150 }
  },
  "border_offset": -1,
  "border": false,
  "border_style": "Rounded",
  "border_implementation": "Komorebi",
  "transparency": false,
  "transparency_alpha": 200,
  "default_workspace_padding": 16,
  "default_container_padding": 16,
  "global_work_area_offset": {"bottom": 18, "left": 0, "right": 0, "top": 22},
  "monitors": [
    {
      "window_based_work_area_offset_limit": 1,
      "window_based_work_area_offset": {"bottom": 0, "left": 500, "right": 1000, "top": 0},
      "workspaces": [
        {
          "name": "I",
          "layout": "UltrawideVerticalStack",
          "custom_layout_rule": {
            "2": "$ENV:KOMOREBI_CONFIG_HOME/custom.json"
          }
        },
        {
          "name": "II",
          "initial_workspace_rules": [
            {
              "kind": "Class",
              "id": "Premiere Pro",
              "matching_strategy": "Equals"
            },
            {
              "kind": "Exe",
              "id": "Adobe Media Encoder.exe"
            },
            {
              "kind": "Exe",
              "id": "AfterFX.exe"
            },
            {
              "kind": "Exe",
              "id": "Adobe Audition.exe"
            }
          ]
        },
        {
          "name": "III",
          "layout": "UltrawideVerticalStack",
          "initial_workspace_rules": [
            {
              "kind": "Exe",
              "id": "Photoshop.exe"
            }
          ]
        },
        {
          "name": "IV",
          "layout": "UltrawideVerticalStack",
          "initial_workspace_rules": [
            {
              "kind": "Exe",
              "id": "Illustrator.exe"
            }
          ]
        },
        {
          "name": "V",
          "layout": "UltrawideVerticalStack",
          "initial_workspace_rules": [
            {
              "kind": "Class",
              "id": "TFruityLoopsMainForm"
            }
          ]
        }
      ]
    },
    {
      "workspaces": [
        {
          "name": "I",
          "layout": "Rows",
          "initial_workspace_rules": [
            {
              "kind": "Exe",
              "id": "Discord.exe"
            },
            {
              "kind": "Exe",
              "id": "YouTube Music.exe"
            },
            {
              "kind": "Exe",
              "id": "Pomodoro Logger.exe"
            },
            {
              "kind": "Exe",
              "id": "obs64.exe"
            }
          ]
        }
      ]
    }
  ],
  "window_hiding_behaviour": "Cloak",
  "object_name_change_title_ignore_list": [],
  "monitor_index_preferences": {},
  "display_index_preferences": {},
  "remove_titlebar_applications": [],
  "floating_window_aspect_ratio": "Standard",
